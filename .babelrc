{
  "presets": ["env", "stage-3", "react"],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".js", ".jsx", ".scss"],
        "alias": {
          "@styles": "./src/styles",
          "@test": "./src/__test__",
          "@pages": "./src/pages"
        }
      }
    ]
  ]
}
